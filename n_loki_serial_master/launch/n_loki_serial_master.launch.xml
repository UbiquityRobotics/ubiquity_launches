<launch>
  <!--Summary: A wrapper for launching the `n_bus_server` launch file.  -->
  <!--Overview: The launch file for this directory starts the `n_bus_server`
      node configured for the Loki platform. -->

  <!-- Convenience arguments: -->
  <arg name="root" value="$(find ubiquity_launches)" />
  <arg name="rab"  value="ros_arduino_bridge" />
  <arg name="bs"   value="bus_server" />

  <include file="$(arg root)/n_$(arg bs)/launch/n_$(arg bs).launch.xml" >
    <arg name="robot_base" value="loki" />
  </include>
</launch>
